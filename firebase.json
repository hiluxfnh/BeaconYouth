{
  "firestore": {
    "rules": "firestore.rules"
  },
  "hosting": {
    "site": "beaconyouth",
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
      {"source": "**/*.@(html|css|js)", "headers": [{"key": "Cache-Control", "value": "public, max-age=600"}]}
    ]
  }
}
